<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.myBatis.myService.dao.BaseCodeDao">
	<select id="selectList" 
		parameterType="com.myBatis.myService.model.BaseCode"
		resultType="com.myBatis.myService.model.BaseCode">
		SELECT 
			  *
		FROM JAIN_EMP.PDIS050T 
		WHERE LOCATE = 'GSH' 
		  AND GRCODE = 'AAA'
	</select>
</mapper>
